{
  "name": "global-mechanic-www",
  "version": "1.0.0",
  "description": "Global Mechanic Website",
  "main": "src-iso",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js 'test/**/*.@(js|jsx)'",
    "test-watch": "npm run test -- --watch",
    "build-scss": "node_modules/.bin/babel-node util/build-index-and-variables-scss.js",
    "build-client": "rm -rf ./dist-client && webpack --config ./config/webpack.production.js",
    "build-iso": "rm -rf ./dist-iso && node_modules/.bin/babel ./src-iso --out-dir ./dist-iso",
    "build-server": "rm -rf ./dist-server && node_modules/.bin/babel ./src-server --out-dir ./dist-server",
    "develop-db": "mongod --dbpath ./storage/data",
    "develop-client": "webpack-dev-server --config ./config/webpack.dev.js",
    "develop-server": "npm run build-server -- --watch & npm run build-iso -- --watch & ./node_modules/.bin/nodemon --watch dist-server  --watch dist-iso ./dist-server ",
    "develop-server:live": "NODE_ENV=production-test; npm run build-server -- --watch & npm run build-iso -- --watch & ./node_modules/.bin/nodemon --watch dist-server  --watch dist-iso ./dist-server ",
    "build": "npm run build-server && npm run build-iso && npm run build-client",
    "start": "node ./dist-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalMechanic/global-mechanic-www.git"
  },
  "author": "Ben Gaumond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GlobalMechanic/global-mechanic-www/issues"
  },
  "homepage": "https://github.com/GlobalMechanic/global-mechanic-www#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.26.4",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "aws-sdk": "^2.24.0",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express-history-api-fallback": "^2.1.0",
    "feathers": "^2.1.1",
    "feathers-authentication": "^0.7.12",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.6.1",
    "feathers-hooks": "^1.8.1",
    "feathers-mongodb": "^2.8.0",
    "feathers-rest": "^1.7.1",
    "feathers-socketio": "^1.5.2",
    "fs-promise": "^2.0.0",
    "immutable": "^3.8.1",
    "is-explicit": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsesc": "^2.4.0",
    "mongodb": "^2.2.24",
    "nedb": "^1.8.0",
    "normalize.css": "^5.0.0",
    "object-assign": "^4.1.1",
    "promise-polyfill": "^6.0.2",
    "promise-queue": "^2.2.3",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-markdown": "^2.4.5",
    "react-router": "^4.0.0-beta.7",
    "react-router-dom": "^4.0.0-beta.7",
    "s3fs": "^2.5.0",
    "serve-favicon": "^2.4.1"
  }
}
